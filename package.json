{
  "name": "formula",
  "version": "1.0.3",
  "description": "Compile formula to JavaScript.",
  "main": "lib/compiler.js",
  "scripts": {
    "build:compiler": "babel src/compiler.js -m commonjs --out-file lib/compiler.js",
    "build:parser": "jison src/parser.jison -m commonjs -o lib/parser.js",
    "build:dist": "browserify ./browserify.js > dist/formula.js && browserify -g uglifyify ./browserify.js > dist/formula.min.js",
    "build": "npm run build:parser && npm run build:compiler && npm run build:dist",
    "test": "tape tests/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FunctionFoundry/formula.git"
  },
  "keywords": [
    "spreadsheet",
    "formula"
  ],
  "author": "Peter W Moresi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FunctionFoundry/formula/issues"
  },
  "homepage": "https://github.com/FunctionFoundry/formula#readme",
  "dependencies": {
    "functionfoundry": "^0.10.20"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.11.5",
    "babel-preset-es2015": "^6.9.0",
    "browserify": "^13.0.1",
    "jison": "^0.4.17",
    "tape": "^4.6.0",
    "uglifyify": "^3.0.2"
  }
}
